if(true){
    console.log('Sempre aparecerei')
}

if(false){
    console.log('Nunca aparecerei')
}

// or/ou: || - Se algum dos termos forem verdadeiros a sentença vai ser verdadeira
console.log(true || true)
console.log(true || false)
console.log(1 < - 20 || 100 <5 || 30 > 10) // apenas um é verdadeiro e toda a sentença da verdadeiro

// and/e: && - So é verdadeira se todas as sentenças forem verdadeiras
console.log(true && true)
console.log(true && false)
console.log(false && true)
console.log(false && false)